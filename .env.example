# ðŸŽ´ DeckLab TCG - Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Pokemon TCG API (pokemontcg.io)
EXPO_PUBLIC_POKEMON_TCG_API_KEY=your_pokemon_tcg_api_key_here

# Google AI (Gemini) for AI Assistant
EXPO_PUBLIC_GOOGLE_API_KEY=your_google_gemini_api_key_here

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# TCGPlayer API (for pricing data)
EXPO_PUBLIC_TCGPLAYER_API_KEY=your_tcgplayer_api_key
EXPO_PUBLIC_TCGPLAYER_BEARER_TOKEN=your_tcgplayer_bearer_token

# eBay API (for auction data)
EXPO_PUBLIC_EBAY_APP_ID=your_ebay_app_id
EXPO_PUBLIC_EBAY_DEV_ID=your_ebay_dev_id
EXPO_PUBLIC_EBAY_CERT_ID=your_ebay_cert_id

# CardMarket API (European market)
EXPO_PUBLIC_CARDMARKET_API_KEY=your_cardmarket_api_key
EXPO_PUBLIC_CARDMARKET_API_SECRET=your_cardmarket_api_secret